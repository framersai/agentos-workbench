# Backend API base URL for local dev
VITE_API_BASE_URL=http://localhost:3001/api

# AgentOS integration
VITE_AGENTOS_BASE_URL=/api/agentos        # goes through Vite dev proxy to localhost:3001
VITE_AGENTOS_ENABLED=true                 # let frontend hit /api/agentos/*
VITE_AGENTOS_CLIENT_MODE=proxy            # keep proxy mode: /api/chat is still primary
VITE_AGENTOS_CHAT_PATH=/agentos/chat
VITE_AGENTOS_STREAM_PATH=/agentos/stream
VITE_AGENTOS_WORKFLOW_DEFINITIONS_PATH=/workflows/definitions
VITE_AGENTOS_PERSONAS_PATH=/personas
VITE_AGENTOS_WITH_CREDENTIALS=true
VITE_AGENTOS_WORKBENCH_USER_ID=agentos-workbench-user

# Leave Supabase/Stripe blank unless you actually use them
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=